#%RAML 1.0 Library

traits:
  paging:
    queryParameters:
      size:
        description: the amount of elements of each result page
        type: integer
        required: false
        example: 10
      page:
        description: the page number
        type: integer
        required: false
        example: 0

types:
  Order:
    type: object
    properties:
      order_id: string
      creation_date: string
